{
	"name": "fintracer-api-fastify",
	"version": "2.0.0",
	"description": "",
	"main": "src/server.js",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "tsx watch src/server.ts",
		"start": "node build/server.mjs",
		"build": "tsup --format esm src --out-dir build",
		"seed": "tsx prisma/seeds.ts",
		"test": "vitest --dir src/use-cases",
		"test:e2e": "vitest --dir src/http",
		"test:create-prisma-environment": "pnpm link --global --dir ./prisma/vitest-environment-prisma",
		"test:install-prisma-environment": "pnpm link --global vitest-environment-prisma",
		"pretest:e2e": "npm-run-all test:create-prisma-environment test:install-prisma-environment"
	},
	"license": "MIT",
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@rocketseat/eslint-config": "^2.2.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/node": "^20.14.7",
		"@types/supertest": "^6.0.2",
		"@vitest/coverage-v8": "^1.6.0",
		"eslint": "8.57.0",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"npm-run-all": "^4.1.5",
		"prisma": "^5.15.1",
		"supertest": "^7.0.0",
		"tsup": "^8.1.0",
		"tsx": "^4.15.6",
		"typescript": "^5.5.2",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"@fastify/cookie": "^9.3.1",
		"@fastify/cors": "9.0.1",
		"@fastify/jwt": "^8.0.1",
		"@prisma/client": "^5.15.1",
		"bcryptjs": "^2.4.3",
		"date-fns": "3.6.0",
		"dayjs": "1.11.11",
		"dotenv": "^16.4.5",
		"fastify": "^4.28.0",
		"zod": "^3.23.8"
	}
}